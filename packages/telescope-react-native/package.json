    "build-macos-universal": "cargo tauri build --target universal-apple-darwin",
    "build-linux-x86": "cargo tauri build --target x86_64-unknown-linux-gnu",
    "build-linux-arm": "cargo tauri build --target aarch64-unknown-linux-gnu",
    "build-all-macos-universal": "node build_all_universal_dmg.js",
    "build-all-windows-x86": "node build_all_windows_x86_exe.js"
