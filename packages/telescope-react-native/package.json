    "tauri:test": "cargo test",
    "lint": "cargo clippy",
    "dev-desktop": "cargo tauri dev",
    "build-windows-x86": "CC_x86_64_pc_windows_msvc=clang CXX_x86_64_pc_windows_msvc=clang++ cargo tauri build --target x86_64-pc-windows-msvc --runner cargo-xwin",
    "build-windows-arm": "CC_aarch64_pc_windows_msvc=clang CXX_aarch64_pc_windows_msvc=clang++ cargo tauri build --target aarch64-pc-windows-msvc --runner cargo-xwin",
